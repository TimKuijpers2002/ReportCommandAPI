{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    },
    "CassandraSettings": {
      "ContactPoint": "{CASSANDRA_CONTACTPOINT}",
      "Port": "{CASSANDRA_PORT}",
      "Keyspace": "{CASSANDRA_KEYSPACE}"
    }
  }
}